config 'global'
	option  anon_swap       '0'
	option  anon_mount      '0'
	option  auto_swap       '1'
	option  auto_mount      '1'
	option  delay_root      '5'
	option  check_fs        '1'

config 'mount'
	option  target  '/mnt'
	option  device  '/dev/sda4'
	option  fstype  'ext4'
	option  enabled_fsck '1'
	option  enabled '1'
